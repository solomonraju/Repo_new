<group name = "Assistants">
	<effect name = "Copy to Grid" id = "Copy to Grid" />
	<source file = "CopyToGrid.jsfl" />
	<allow types = "button, shape, movie clip, graphic, text, bitmap" />

	<properties>
		<property name="Rows" variable="rows" min="1" defaultValue="2" type="Number" />
		<property name="Columns" variable="columns" min="1" defaultValue="2" type="Number" />
		<property name="Horizontal Gap" variable="horizontalGap" min="0" defaultValue="2" type="Number" />
		<property name="Vertical Gap" variable="verticalGap" min="0" defaultValue="2" type="Number" />
		<property name="First" variable="first" defaultValue="1" type="Number" />
	</properties>

	<!-- buttons="accept, cancel"> -->
	<dialog id="copytogrid-dialog" title="Copy To Grid">
		<flash id="copytogrid_ui" src="copytogrid.swf" width="772" height="456">
			<property id="first" />
			<property id="rows" />
			<property id="columns" />
			<property id="horizontalGap" />
			<property id="verticalGap" />
		</flash>
	</dialog>

</group>

